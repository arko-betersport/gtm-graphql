type Query {
    getGtm: Gtm @resolver(class:"\\ScandiPWA\\GtmGraphQl\\Model\\Resolver\\GetGtm")
}

type Gtm {
    enabled: Boolean
    gtm_id: String
    events: GtmEvents
}

type GtmEvents {
    general: Boolean
    productImpression: Boolean
    productClick: Boolean
    addToCart: Boolean
    removeFromCart: Boolean
    productDetail: Boolean
    purchase: Boolean
    checkout: Boolean
    checkoutOption: Boolean
    userLogin: Boolean
    userRegister: Boolean
    notFound: Boolean
    categoryFilters: Boolean
    additional: Boolean
}
